#!/bin/sh

getenv=True
script_dir=$ENV(tool)/misc/
log_dir=$ENV(data)/condor_logs/setup/md5/
input_dir=$ENV(data)/fastqs_from_core/fastqs

# Program
executable=/usr/bin/Rscript

# Processor cores requested
request_cpus = 1

# Memory requested
request_memory = 16 GB

notify_user = hortowe@ohsu.edu

# Arguments 

output=$(log_dir)stdout_process.md5.out
error=$(log_dir)stderr_process.md5.out
log=$(log_dir)condor_process.md5.log
arguments=$(script_dir)process.md5.R $(input_dir)/
queue 1
