#!/bin/sh

getenv=True
script_dir=$ENV(tool)/misc/
log_dir=$ENV(data)/condor_logs/combine/

# Program
executable=/bin/sh

# Processor cores requested
request_cpus = 1

# Memory requested
request_memory = 64 GB

notify_user = hortowe@ohsu.edu

# Arguments 

output=$(log_dir)stdout_combine.out
error=$(log_dir)stderr_cobmine.out
log=$(log_dir)condor_combine.log
arguments=$(script_dir)combine_replicates.sh 35 35
queue 1
